# Azure Environment Configuration
# Copy this file to .env and update the values

# Azure Developer CLI Configuration
AZURE_ENV_NAME=haime-dev
AZURE_LOCATION=eastus
AZURE_SUBSCRIPTION_ID=

# Optional: Specify a specific resource group (will be created if it doesn't exist)
# AZURE_RESOURCE_GROUP_NAME=rg-haime-dev

# Azure OpenAI Configuration
# Set CREATE_AZURE_OPENAI=false to use existing Azure OpenAI service
CREATE_AZURE_OPENAI=true

# If using existing Azure OpenAI service (when CREATE_AZURE_OPENAI=false)
# EXISTING_AZURE_OPENAI_ENDPOINT=https://your-openai-service.openai.azure.com/
# EXISTING_AZURE_OPENAI_KEY=your-api-key

# Azure OpenAI Model Configuration
AZURE_OPENAI_DEPLOYMENT=o3-mini
AZURE_OPENAI_VERSION=2025-01-01-preview
AZURE_OPENAI_MODEL_NAME=o3-mini
AZURE_OPENAI_MODEL_VERSION=

# Function App Configuration
ENABLE_EVALUATOR_ADDON=true
DOCKER_IMAGE_TAG=latest

# Application Configuration
# These will be populated automatically after deployment
REACT_APP_CLIENT_ID=
REACT_APP_TENANT_ID=
REACT_APP_API_BASE_URL=

# Development URLs (for local testing)
REACT_APP_API_BASE_URL_DEV=http://localhost:5000 