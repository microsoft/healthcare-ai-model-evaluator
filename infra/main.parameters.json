{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "keyVaultLocation": {
      "value": "${AZURE_KEYVAULT_LOCATION}"
    },
    "cosmosLocation": {
      "value": "${AZURE_COSMOS_LOCATION}"
    },
    "containerRegistryLocation": {
      "value": "${AZURE_CONTAINER_REGISTRY_LOCATION}"
    },
    "storageLocation": {
      "value": "${AZURE_STORAGE_LOCATION}"
    },
    "functionsLocation": {
      "value": "${AZURE_FUNCTIONS_LOCATION}"
    },
    "staticWebAppLocation": {
      "value": "${AZURE_STATIC_WEB_APP_LOCATION}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "apiImageName": {
      "value": "${SERVICE_API_IMAGE_NAME}"
    },
    "openAIServiceName": {
      "value": "${AZURE_OPENAI_SERVICE_NAME=}"
    },
    "cosmosAccountName": {
      "value": "${AZURE_COSMOS_ACCOUNT_NAME=}"
    },
    "containerRegistryName": {
      "value": "${AZURE_CONTAINER_REGISTRY_NAME=}"
    },
    "storageAccountName": {
      "value": "${AZURE_STORAGE_ACCOUNT_NAME=}"
    },
    "keyVaultName": {
      "value": "${AZURE_KEY_VAULT_NAME=}"
    },
    "containerAppsEnvName": {
      "value": "${AZURE_CONTAINER_APPS_ENV_NAME=}"
    },
    "staticWebAppName": {
      "value": "${AZURE_STATIC_WEB_APP_NAME=}"
    },
    "logAnalyticsName": {
      "value": "${AZURE_LOG_ANALYTICS_NAME=}"
    },
    "applicationInsightsName": {
      "value": "${AZURE_APPLICATION_INSIGHTS_NAME=}"
    },
    "createOpenAI": {
      "value": "${CREATE_AZURE_OPENAI=true}"
    },
    "existingOpenAIEndpoint": {
      "value": "${EXISTING_AZURE_OPENAI_ENDPOINT=}"
    },
    "existingOpenAIKey": {
      "value": "${EXISTING_AZURE_OPENAI_KEY=}"
    },
    "model": {
      "value": "${AZURE_OPENAI_MODEL}"
    },
    "modelCapacity": {
      "value": "${AZURE_OPENAI_MODEL_CAPACITY}"
    },
    "modelSku": {
      "value": "${AZURE_OPENAI_MODEL_SKU}"
    },
    "gptDeploymentLocation": {
      "value": "${AZURE_GPT_LOCATION}"
    },
    "openAIApiVersion": {
      "value": "${AZURE_OPENAI_VERSION=2025-01-01-preview}"
    },
    "enableEvaluatorAddon": {
      "value": "${ENABLE_EVALUATOR_ADDON}"
    },
    "dockerImageTag": {
      "value": "${DOCKER_IMAGE_TAG=latest}"
    },
    "enableAcs": {
      "value": "${ENABLE_ACS=true}"
    },
    "webBaseUrl": {
      "value": "${WEB_BASE_URL=}"
    },
    "emailFrom": {
      "value": "${EMAIL_FROM=no-reply@haime.local}"
    },
    "emailSmtpHost": {
      "value": "${EMAIL_SMTP_HOST=}"
    },
    "emailSmtpPort": {
      "value": "${EMAIL_SMTP_PORT=587}"
    },
    "emailSmtpUser": {
      "value": "${EMAIL_SMTP_USER=}"
    },
    "emailSmtpPass": {
      "value": "${EMAIL_SMTP_PASS=}"
    },
    "emailSmtpUseSsl": {
      "value": "${EMAIL_SMTP_USESSL=true}"
    },
    "emailEnabled": {
      "value": "${EMAIL_ENABLED=false}"
    },
    "createAdminUser": {
      "value": "${CREATE_ADMIN_USER=true}"
    },
    "tags": {
      "value": {
        "Owner": "AI Team",
        "Project": "healthcare-ai-model-evaluator",
        "Toolkit": "Bicep"
      }
    }
  }
} 